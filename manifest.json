{
  "name": "Python Requests Export",
  "description": "Copy requests from devtools network panel as python requests code",
  "version": "1.0",
  "author": "Jannik Hartung",
  "manifest_version": 2,
  "minimum_chrome_version": "40.0",
  "devtools_page": "devtools.html",
  "options_ui": {
    "browser_style": true,
    "page": "options.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Requests Export to Python",
    "default_popup": "popup.html"
  }
}