{
  "name": "Python Requests Export",
  "description": "Copy requests from devtools network panel as python requests code",
  "version": "1.0",
  "author": "Jannik Hartung",
  "manifest_version": 3,
  "icons": {
    "16": "icons/logo-16.png",
    "32": "icons/logo-32.png",
    "48": "icons/logo-48.png",
    "96": "icons/logo-96.png",
    "128": "icons/logo-128.png"
  },
  "minimum_chrome_version": "40.0",
  "devtools_page": "devtools.html",
  "options_ui": {
    "page": "options.html",
    "browser_style": true,
    "chrome_style": true,
    "open_in_tab": true
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_title": "Requests Export to Python",
    "default_popup": "popup.html"
  }
}
